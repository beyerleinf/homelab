- Storage:
    - Seafile:
        icon: "sh-seafile"
        href: https://files.lab.beyerleinf.de
        siteMonitor: https://files.lab.beyerleinf.de
        server: arya
        container: seafile

    - Paperless:
        icon: "sh-paperless-ngx"
        href: https://paperless.lab.beyerleinf.de
        siteMonitor: https://paperless.lab.beyerleinf.de
        server: arya
        container: seafile
        widget:
          type: paperlessngx
          url: https://paperless.lab.beyerleinf.de
          key: {{ paperless_api_key }}

    - Immich:
        icon: "sh-immich"
        href: https://pics.lab.beyerleinf.de
        siteMonitor: https://pics.lab.beyerleinf.de
        server: arya
        container: immich-server
        widget:
          type: immich
          url: https://pics.lab.beyerleinf.de
          key: {{ immich_api_key }}

- Media:
    - Jellyfin:
        icon: "sh-jellyfin"
        href: https://media.lab.beyerleinf.de
        siteMonitor: https://media.lab.beyerleinf.de/health
        server: arya
        container: jellyfin
        widget:
          type: jellyfin
          url: https://media.lab.beyerleinf.de
          key: {{ jellyfin_api_key }}
          enableBlocks: true
          enableNowPlaying: true
          enableUser: true
          showEpisodeNumber: true
          expandOneStreamToTwoRows: false
    
    - "Tube Archivist":
        icon: "sh-tube-archivist"
        href: "https://ta.lab.beyerleinf.de"
        siteMonitor: "https://ta.lab.beyerleinf.de"
        server: arya
        container: "tubearchivist"
        widget:
          type: "tubearchivist"
          url: "https://ta.lab.beyerleinf.de"
          key: {{ tube_archivist_api_key }}

- Tools:
    - PiHole:
        icon: "sh-pi-hole"
        href: http://10.0.0.222:8088
        siteMonitor: http://10.0.0.222:8088
        server: arya
        container: pihole
        widget:
          type: pihole
          url: http://10.0.0.222:8088
          version: 5
          key: {{ pihole_api_key }}

    - UniFi:
        icon: "sh-ubiquiti-unifi"
        href: "https://unifi.lab.beyerleinf.de"
        siteMonitor: https://unifi.lab.beyerleinf.de
        server: arya
        container: unifi-network-application
        widget:
          type: unifi
          url: https://unifi.lab.beyerleinf.de
          username: {{ unifi_username }}
          password: {{ unifi_password }}

    - Umami:
        icon: "sh-umami"
        href: https://umami.lab.beyerleinf.de
        siteMonitor: https://umami.lab.beyerleinf.de
        server: arya
        container: umami

- "Personal Finance":
    - "Firefly III":
        icon: "sh-firefly-iii"
        href: https://firefly.lab.beyerleinf.de
        siteMonitor: https://firefly.lab.beyerleinf.de
        server: arya
        container: firefly
    - Ghostfolio:
        icon: "sh-ghostfolio"
        href: https://ghostfolio.lab.beyerleinf.de
        siteMonitor: https://ghostfolio.lab.beyerleinf.de
        server: arya
        container: ghostfolio
        widget:
          type: ghostfolio
          url: https://ghostfolio.lab.beyerleinf.de
          key: {{ ghostfolio_token }}